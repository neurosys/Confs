
alias ll='ls -lGah --color=auto'

# Display the size (in MB) each item (file or folder) in this folder takes up and display them in a sorted order
alias lu='find -maxdepth 1 -exec du -sm {} \; | sed -e "s;^\([0-9]\+\)\t\+;\1 M\t;" | sort -rn'

alias wget='wget -nc --user-agent="Mozilla/5.0 (Macintosh; Intel Mac OS X 10_8_2) AppleWebKit/537.17 (KHTML, like Gecko) Chrome/24.0.1309.0 Safari/537.17" -m --convert-links --page-requisites --no-parent -e robots=off'
# -nc no clobber (Don't overwrite an already file, nor save it as file.nr1 nr2 ... 
# --wait=20 wait 20 seconds between requests
# --random-wait randomize the wait between 0.5 * wait - 1.5 * wait time
# --user-agent=  Name of the user-agent 
# --convert-links after download convert links for local usage
# -m mirrors the url
# --page-requisites load the resources necesarry to view the page as it is on the web
# --no-parent don't ascend to parent directory
# -e robots=off ignore the contents of robots.txt






# Aliases for the yocto build
alias cdcomp32='cd ~/trunk/QorIQ-SDK-yocto/build_t1040rdb_release; source SOURCE_THIS'
alias cdcomp64='cd ~/trunk/QorIQ-SDK-yocto/build_t1040rdb-64b_release; source SOURCE_THIS' 
alias cdimg32='cdcomp32;cd tmp/deploy/images/t1040rdb; ll' 
alias cdimg64='cdcomp64;cd tmp/deploy/images/t1040rdb; ll' 
alias cdrpm32='cdcomp32;cd tmp/deploy/rpm; ll ' 
alias cdrpm64='cdcomp64;cd tmp/deploy/rpm; ll ' 
alias compall32='cdcomp32;bitbake fsl-image-core' 
alias compall64='cdcomp64;bitbake fsl-image-core' 
alias compkernel32='cdcomp32;bitbake virtual/kernel' 
alias compkernel64='cdcomp64;bitbake virtual/kernel' 
alias kernelconfig32='cdcomp32;bitbake virtual/kernel -c menuconfig' 
alias kernelconfig64='cdcomp64;bitbake virtual/kernel -c menuconfig' 
alias cdubootsrc='cdcomp;cd tmp/work/t1040rdb-fsl-linux/u-boot/2014.01+fslgit-r0/git' 
alias cdkernsrc='cdcomp;cd tmp/work/t1040rdb-fsl-linux/linux-qoriq-sdk/3.12-r0/git' 

alias rpmlist='rpm -qlp '


if [ $is_cygwin ]
then 
    unset GREP_OPTIONS
else 
    export GREP_OPTIONS="-Rn --color=auto"
fi

if [ -f ${confs_folder}/.cfgproxy ]
then
    export http_proxy=$(cat ${confs_folder}/.cfgproxy)
fi


if [ $is_cygwin ]
then
    export EDITOR=runemacs.exe
else
    export EDITOR=emacs
fi

# In order for the git gui to work on CYGWIN
if [ $is_cygwin ]
then
    export DISPLAY=:0.0
fi

{
    // The scope in every section can be found by presing Ctrl+Shift+p an typing in the textbox:
    // "Developer: Inspect Editor Tokens ans Scopes" Use the "textmate scopes"
    // maybe take a look at this theme editor here: https://themes.vscode.one/edit/local/
	"name": "myElflord",
	"colors": {
		// Editor
		"editor.background": "#202020",
		"editor.foreground": "#00ffff",
		// Activity Bar
		"activityBarBadge.background": "#007acc",
		"activityBar.background": "#000000",
		// Status Bar 
		"statusBar.background": "#404040",
		"statusBar.foreground": "#ffffff",
		"statusBarItem.remoteBackground": "#202020",
		"statusBarItem.remoteForeground": "#ffffff",
		// Sidebar
		"sideBar.background": "#181818",
		"sideBar.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.foreground": "#ffffff",
		"sideBarTitle.foreground": "#00ffff",
		// Line Numbers
		"editorLineNumber.foreground": "#7f7f7f",
		"editorLineNumber.activeForeground": "#00ffff",
		// Cursor
		"editorCursor.background": "#000000",
		"editorCursor.foreground": "#ffffff",
		// Title Bar
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#ffffff",
		// Tabs
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroupHeader.tabsForeground": "#000000",
		"tab.inactiveBackground": "#404040",
		"tab.inactiveForeground": "#ffffff",
		// Buttons
		"button.background": "#ffffff",
		"button.foreground": "#000000"
	},
	"tokenColors": [
        {
            "name": "cpp.colon-colon",
            "scope": [
                "punctuation.separator.namespace.access.cpp"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "cpp.angle-brackets",
            "scope": [
                "punctuation.section.angle-brackets.begin.template.call.cpp",
                "punctuation.section.angle-brackets.end.template.call.cpp"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "cpp.semicol",
            "scope": [
                "punctuation.terminator.statement.cpp"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "cpp.dot",
            "scope": [
                "punctuation.separator.dot-access.cpp",
                "punctuation.separator.pointer-access.cpp",
                "punctuation.separator.colon.range-based.cpp",
                "punctuation.section.arguments.end.bracket.round.function.member.cpp",
                "punctuation.section.arguments.begin.bracket.round.function.member.cpp",
                "keyword.operator.bitwise.shift.cpp",
                "punctuation.separator.delimiter.comma.cpp"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "cpp.static",
            "scope": [
                "storage.modifier.static.cpp",
                "storage.modifier.specifier.static.cpp"
            ],
            "settings": {
                "foreground": "#87ffaf"
            }
        },
        {
            "name": "cpp.const",
            "scope": [
                "storage.modifier.specifier.const.cpp"
            ],
            "settings": {
                "foreground": "#87ffaf"
            }
        },
        {
            "name": "cpp.using",
            "scope": [
                "storage.modifier.using.cpp",
                "keyword.other.using.directive.cpp"
            ],
            "settings": {
                "foreground": "#87ffaf"
            }
        },
        {
            "name": "cpp.bool_values",
            "scope": [
                "constant.language.true.cpp",
                "constant.language.false.cpp"
            ],
            "settings": {
                "foreground": "#87ffaf"
            }
        },
        {
            "name": "cpp.virtual",
            "scope": [
                "storage.modifier.specifier.functional.pre-parameters.virtual.cpp"
            ],
            "settings": {
                "foreground": "#87ffaf"
            }
        },
		{
			"name": "markdown.h4",
			"scope": [
				"heading.4.markdown"
			],
			"settings": {
				"foreground": "#FFF000"
			}
		},
		{
			"name": "markdown.h3",
			"scope": [
				"heading.3.markdown"
			],
			"settings": {
				"foreground": "#00AFFF"
			}
		},
		{
			"name": "markdown.h2",
			"scope": [
				"heading.2.markdown"
			],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "markdown.h1",
			"scope": [
				"heading.1.markdown"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "markdown.code",
			"scope": [
				"meta.embedded.block",
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Terraform.resource.keyword",
			"scope": [
                "entity.name.type.terraform"
			],
			"settings": {
				"foreground": "#FFF000"
			}
		},
		{
			"name": "Terraform.resource.constants",
			"scope": [
                "constant.language.hcl"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Docker.resource.keyword",
			"scope": [
                "keyword.other.special-method.dockerfile"
			],
			"settings": {
				"foreground": "#FFF000"
			}
		},
        {
            "name": "XmlText",
            "scope": [
                "text.xml"
            ],
			"settings": {
				"foreground": "#FFFFFF"
			}
        },
        {
            "name": "XmlTag",
            "scope": [
                "entity.name.tag.localname.xml"
            ],
			"settings": {
				"foreground": "#00FFFF"
			}
        },
        {
            "name": "XmlAttribute",
            "scope": [
                "entity.other.attribute-name.localname.xml"
            ],
			"settings": {
				"foreground": "#00FF00"
			}
        },
		{
			"name": "Yaml.Name.Tag",
			"scope": [
                "entity.name.tag.yaml"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Yaml.Name.Separator",
			"scope": [
                "punctuation.separator.key-value.mapping.yaml",
                "punctuation.definition.block.sequence.item.yaml"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Yaml.Value",
			"scope": [
                "string.unquoted.plain.out.yaml"
			],
			"settings": {
				"foreground": "#00AFFF"
			}
		},
		{
			"name": "Strings",
			"scope": [
                "string.quoted",
                "string.unquoted.heredoc.expanded.shell"
			],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Numbers",
			"scope": [
                "constant.numeric"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
                "punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#80a0ff"
			}
		},
		{
			"name": "Conditional",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
        {
            "name": "Operator",
            "scope": [
                "keyword.operator"
            ],
			"settings": {
				"foreground": "#ff0000"
			}
        },
        {
            "name": "Types",
            "scope": [
                "storage.type"
            ],
			"settings": {
				"foreground": "#87ffaf"
			}
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.function.go",
                "keyword.const.go",
                "keyword.var",
                "keyword.import.go",
                "keyword.package.go",
                "keyword.type.go",
                "keyword.interface.go",
                "keyword.struct.go",
                "constant.language.go",
                "support.function.builtin.go"
            ],
            "settings": {
				"foreground": "#ffff00"
            }
        },
		{
			"name": "Type",
			"scope": [
				"meta.type"
			],
			"settings": {
				"foreground": "#60ff60"
			}
		},
		{
			"name": "FunctionCall",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "FunctionNameDefinition",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.other.comma",
				"punctuation.other.period",
				"punctuation.other.colon",
				"punctuation.definition.bracket.square",
				"punctuation.definition.end.bracket.round",
				"punctuation.definition.end.bracket.curly",
				"punctuation.definition.begin.bracket.round",
				"punctuation.definition.begin.bracket.curly",
				"punctuation.definition.begin.bracket.curly.go",
				"punctuation.definition.end.bracket.curly.go",
				"punctuation.definition.begin.bracket.round.go",
				"punctuation.definition.end.bracket.round.go",
                "keyword.operator.assignment",
                "keyword.operator.address"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Search",
			"scope": [
				"entity.name.function.search"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "Todo",
			"scope": [
				"meta.todo"
			],
			"settings": {
				"foreground": "#ffff00"
			}
		},
        {
            "name": "RedTokensInJavascript",
			"scope": [
				"punctuation.separator.key-value.js",
				"punctuation.separator.comma.js",
				"punctuation.terminator.statement.js",
				"punctuation.accessor.js",
                "constant.language.null.js"
			],
			"settings": {
				"foreground": "#ff0000"
			}
        }
	]
}
		
